{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "API_ID": "@api-id",
    "API_HASH": "@api-hash",
    "BOT_TOKEN": "@bot-token",
    "MONGO_URI": "@mongo-uri"
  },
  "github": {
    "enabled": true,
    "autoJobCancelation": true,
    "silent": false
  }
}
